<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/tv_time_from"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="10:45 AM"
        android:textColor="@color/grey_dark_text"
        android:textSize="@dimen/text_size_doctor_profile_item_header_child" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:text="@string/to_with_spaces"
        android:textColor="@color/black"
        android:textSize="@dimen/text_size_doctor_profile_item_header_child" />

    <TextView
        android:id="@+id/tv_time_to"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="10:45 PM"
        android:textColor="@color/grey_dark_text"
        android:textSize="@dimen/text_size_doctor_profile_item_header_child" />
</LinearLayout>