                                android:paddingBottom="@dimen/layout_padding"
                                android:singleLine="true"
                                android:text="@string/food"
                                android:textColor="@color/orange"
                                android:textColorHint="@color/grey_hint_color"
                                android:textSize="@dimen/text_size_floating_label"
                                android:textStyle="bold" />

                            <com.healthcoco.healthcocopad.views.TextViewFontAwesome
                                android:id="@+id/tv_add_evening_snack"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:paddingLeft="@dimen/item_contact_padding"
                                android:paddingTop="@dimen/layout_padding"
                                android:paddingRight="@dimen/item_contact_padding"
                                android:paddingBottom="@dimen/layout_padding"
                                android:text="@string/icon_add"
                                android:textColor="@color/orange"
                                android:textSize="@dimen/text_size_treatment_list_title" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin_left_right_add_vital_signs"
                            android:layout_marginRight="@dimen/margin_left_right_add_vital_signs"
